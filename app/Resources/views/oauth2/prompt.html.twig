{% extends 'base.html.twig' %}


{% block stylesheets %}
	{{ parent()}}

	<style>
		
		.form-container{
			margin-top: 20px;
		}

		#header{
			background: #444;
			padding: 10px;
			min-height: 60px;
			display: flex;
			flex-direction: row;
			color: #fff;
			align-items: center;
		}
	</style>
{% endblock %}

{% block body %}
<div id="header">
	<h1>Test Oauth2</h1>

	<div style="margin-left: auto;color: #fff">
		<h3>{{ app.user.username }}</h3>
		<small>{{ app.user.email }}</small>
	</div>
</div>

<div class="container">
	<div class="row justify-content-center">
		

		<div class="col-md-8">
			<form method="POST" action="{{ path('oauth2_authorize_prompt',app.request.query.all()) }}" class="jumbotron form-container">
				<h1 class="display-5">Autorisation d'application</h1>
				
				<p class="lead">
					<strong>{{ client.name }}</strong> souhaiterait avoir votre permission pour acceder Ã  certaines informations de votre compte.
				</p>
				
				<hr class="my-4">
				
				<p>
					<div class="row">
						<div class="col-md-6">
							<h4>Permissions requises</h4>

							<div>
								<ul>
									{% for item in scopes %}
								        <li>
								        	{{ item.name }}
								        	<div>
								        		<small>
								        			{{ item.description }}
								        		</small>
								        	</div>
								        </li>
								    {% endfor %}
								</ul>
							</div>
						</div>

						<div class="col-md-6">
							<div class="card">
								<img class="card-img-top" src="data:image/jpeg;base64," alt="Card image cap">
							  	<div class="card-body">
							  		<p class="card-title">
							    		<h3>{{client.name}}</h3>
							    	</p>
							    	<p class="card-text" style="padding: 10px">
							    		{{client.description}}
							    	</p>
							  	</div>
							</div>
						</div>
					</div>
				</p>
				 
				<p class="lead">

				    <button name="user_action" value="grant" class="btn btn-secondary"  type="submit">
				    	<i class="fa fa-check"></i> Accepter
				    </button>

				    <button name="user_action" value="deny" class="btn btn-secondary"  type="submit">
				    	<i class="fa fa-times"></i> Refuser
				    </button>
				</p>
			</form>
		</div>

	</div>
</div>
{% endblock %}